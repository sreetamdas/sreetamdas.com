    "nitro": "3.0.1-alpha.2",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-icons": "^5.5.0",
    "react-wrap-balancer": "^1.1.1",
    "rehype-raw": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-toc": "^9.0.0",
    "safe-mdx": "^1.3.9",
    "shiki-magic-move": "^1.2.1",
    "type-fest": "^5.4.3",
    "unist-util-visit": "^5.1.0",
    "vite": "^7.3.1",
