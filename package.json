  "browser": {
    "fs": false
  },
  "msw": {
    "workerDirectory": "public"
  },
  "engines": {
    "node": "22"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "esbuild",
      "sharp",
      "workerd"
    ]
  }
}
