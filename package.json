{
	"name": "sreetamdas.com",
	"version": "5.0.0",
	"type": "module",
	"license": "MIT",
	"scripts": {
		"dev:content": "velite --config './.config/velite.config.ts' --watch",
		"dev:next:turbo": "next dev -p 5045 --turbo",
		"dev:next": "next dev -p 5045",
		"build:content": "velite --config './.config/velite.config.ts' --clean",
		"build:next": "next build",
		"analyze": "ANALYZE=true next build",
		"dev": "pnpm run dev:content & vite dev",
		"prebuild": "pnpm run build:content",
		"build": "vite build",
		"preview": "vite preview",
		"export": "next build && next export",
		"compile": "tsc --watch",
		"typecheck": "tsc -p tsconfig.json",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./swrc/cloudflare-env.d.ts",
		"eslint:check": "eslint --config ./.config/eslint.config.mjs ./src",
		"eslint:fix": "eslint --config ./.config/eslint.config.mjs --fix ./src",
		"oxlint:check": "oxlint --config ./.config/.oxlintrc.json ./src",
		"prettier:check": "prettier --config ./.config/prettier.config.mjs --ignore-path ./.config/.prettierignore --check ./src",
		"prettier:write": "prettier --config ./.config/prettier.config.mjs --ignore-path ./.config/.prettierignore --write ./src",
		"lint": "pnpm run prettier:check && pnpm run eslint:check",
		"lint:fix": "pnpm run prettier:write && pnpm run eslint:fix",
		"format": "pnpm run prettier:write .",
		"deploy": "pnpm run build && wrangler deploy"
	},
	"dependencies": {
		"@cloudflare/vite-plugin": "^1.13.12",
		"@fontsource-variable/bricolage-grotesque": "^5.2.10",
		"@fontsource-variable/inter": "^5.2.8",
		"@mdx-js/mdx": "^3.1.1",
		"@plausible-analytics/tracker": "^0.4.3",
		"@radix-ui/react-alert-dialog": "^1.1.15",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-switch": "^1.2.6",
		"@radix-ui/react-toast": "^1.2.15",
		"@radix-ui/react-visually-hidden": "^1.2.3",
		"@shikijs/colorized-brackets": "^3.13.0",
		"@shikijs/langs-precompiled": "^3.13.0",
		"@sreetamdas/karma": "^3.1.1",
		"@supabase/sentry-js-integration": "^0.3.0",
		"@supabase/supabase-js": "^2.75.0",
		"@tailwindcss/vite": "^4.1.14",
		"@tanstack/react-query": "^5.90.2",
		"@tanstack/react-query-devtools": "^5.90.2",
		"@tanstack/react-router": "^1.132.47",
		"@tanstack/react-start": "^1.132.51",
		"@tanstack/start-static-server-functions": "^1.132.48",
		"cloudflare": "^5.2.0",
		"clsx": "^2.1.1",
		"image-size": "^2.0.2",
		"lodash-es": "^4.17.21",
		"next-plausible": "^3.12.4",
		"react": "19.2.0",
		"react-dom": "19.2.0",
		"react-icons": "^5.5.0",
		"react-wrap-balancer": "^1.1.1",
		"rehype-raw": "^7.0.0",
		"rehype-slug": "^6.0.0",
		"remark-frontmatter": "^5.0.0",
		"remark-gfm": "^4.0.1",
		"remark-toc": "^9.0.0",
		"shiki-magic-move": "^1.2.0",
		"type-fest": "^5.0.1",
