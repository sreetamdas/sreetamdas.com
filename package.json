		"vite-tsconfig-paths": "^5.1.4",
		"wrangler": "^4.42.2"
	},
	"browser": {
		"fs": false
	},
	"msw": {
		"workerDirectory": "public"
	},
	"engines": {
		"node": "22"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@parcel/watcher",
			"@tailwindcss/oxide",
			"esbuild",
			"sharp",
			"workerd"
		]
	}
