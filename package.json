		"@types/eslint": "~9.6.1",
		"@types/lodash-es": "^4.17.12",
		"@types/mdx": "^2.0.13",
		"@types/node": "^24.7.1",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.1",
		"@types/unist": "~3.0.3",
		"@vitejs/plugin-react": "^5.0.4",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.37.0",
		"eslint-plugin-mdx": "^3.6.2",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"globals": "^16.4.0",
		"next-sitemap": "^4.2.3",
