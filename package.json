{
  "name": "sreetamdas.com",
  "version": "5.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev:content": "velite --config './.config/velite.config.ts' --watch",
    "build:content": "velite --config './.config/velite.config.ts' --clean",
    "dev": "pnpm run dev:content & vite dev",
    "prebuild": "pnpm run build:content",
    "build": "vite build",
    "preview": "vite preview",
    "compile": "tsc --watch",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./src/cloudflare-env.d.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply sreetamdas_com --local",
    "db:migrate:remote": "wrangler d1 migrations apply sreetamdas_com --remote",
    "migrate:pageviews:export-supabase": "node scripts/supabase-export-page-details.mjs",
    "migrate:pageviews:generate-d1-sql": "node scripts/d1-generate-import-sql.mjs",
    "oxlint:check": "oxlint --config ./.config/.oxlintrc.json ./src",
    "oxlint:fix": "oxlint --config ./.config/.oxlintrc.json --fix ./src",
    "oxfmt:check": "oxfmt --config ./.config/.oxfmtrc.json --ignore-path ./.config/.oxfmtignore --check ./src '!**/routeTree.gen.ts'",
    "oxfmt:write": "oxfmt --config ./.config/.oxfmtrc.json --ignore-path ./.config/.oxfmtignore --write ./src '!**/routeTree.gen.ts'",
    "lint": "pnpm run oxfmt:check && pnpm run oxlint:check",
    "lint:fix": "pnpm run oxfmt:write && pnpm run oxlint:fix",
    "format": "pnpm run oxfmt:write",
    "deploy": "pnpm run build && wrangler deploy"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "^1.17.0",
    "@fontsource-variable/bricolage-grotesque": "^5.2.10",
    "@fontsource-variable/inter": "^5.2.8",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-visually-hidden": "^1.2.4",
    "@shikijs/colorized-brackets": "^3.19.0",
    "@shikijs/langs-precompiled": "^3.19.0",
    "@sreetamdas/karma": "^3.1.1",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@tanstack/react-router": "^1.140.5",
    "@tanstack/react-start": "^1.140.5",
    "@notionhq/client": "^5.4.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.5",
    "image-size": "^2.0.2",
    "lodash-es": "^4.17.21",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-icons": "^5.5.0",
    "rehype-raw": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-toc": "^9.0.0",
    "safe-mdx": "^1.3.9",
    "shiki": "^3.19.0",
    "shiki-magic-move": "^1.2.1",
    "tailwind-merge": "^3.4.0",
    "unist-util-visit": "^5.0.0",
    "vite": "^7.2.7",
    "zod": "^4.1.13",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@octokit/types": "^16.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/mdx": "^2.0.13",
    "@types/node": "^25.0.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/unist": "~3.0.3",
    "@vitejs/plugin-react": "^5.1.2",
    "drizzle-kit": "^0.31.4",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.32.0",
    "oxlint-tsgolint": "^0.11.5",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "velite": "^0.3.1",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.53.0"
  },
  "browser": {
    "fs": false
  },
  "msw": {
    "workerDirectory": "public"
  },
  "engines": {
    "node": "22"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "esbuild",
      "sharp",
      "workerd"
    ]
  }
}
